    "from sympy import *\n",
    "from sympy.abc import x, y, z\n",
    "\n",
    "# LogSumExp\n",
    "f = log(exp(x) + exp(y) + exp(z))\n",
    "display(f)\n",
    "\n",
    "# Gradient\n",
    "g = Matrix([diff(f, x), diff(f, y), diff(f, z)])\n",
    "display(g)\n",
    "\n",
    "# Hessian\n",
    "h = hessian(f, (x, y, z))\n",
    "display(h)"
